package prodcons.v7;

public class TestTaskExecutor {

    public static void main(String[] args) throws Exception {

        TaskExecutor exec = new TaskExecutor(5);

        // 2 producteurs × 10 tâches chacun
        new TaskProducer(exec, 10).start();
        new TaskProducer(exec, 10).start();

        Thread.sleep(2000);

        System.out.println("FIN TEST TaskExecutor politique 1");
    }
}